{
    "name": "JZT",
    "description": "A DOS-era Adventure Game for the Web",
    "version": "1.0.2",
    "author": {
        "name": "Mark McIntyre"
    },
    "devDependencies": {
        "browserify": "~12.0.1"
    },
    "scripts": {
        "build": "{echo wtf && cat ./script/thirdparty/lz-string-1.3.3.js && browserify -d ./script/jzt.js -s jzt } > ./build/jzt.min.js",
        "build-production": "cat ./script/thirdparty/lz-string-1.3.3.js && browserify -jzt.js -s jzt | uglifyjs > ./build/jzt.min.js",
    }
}
